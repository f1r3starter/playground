{% extends 'Layout.html.twig' %}

{% block content %}
    {% include 'FlashMessages.html.twig' %}
    <form method="post" action="">
        <label for="submission-title">Title</label>
        <input type="text" id="submission-title" name="title"><br>
        <label for="submission-url">Url</label>
        <input type="text" id="submission-url" name="url"><br>
        <input type="hidden" name="token" value="{{ get_token('submission') }}">
        <input type="submit">
    </form>
{% endblock %}